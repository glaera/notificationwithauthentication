{
  "name": "notification-getting-started",
  "version": "1.0.0",
  "description": "notifications",
  "repository": "https://github.com/glaera/notificationwithauthentication",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "test": "repo-tools test app && ava -t 30s --tap test/*.test.js",
    "e2e": "repo-tools test deploy && E2E_TESTS=True ava -t 30s --tap test/*.test.js",
    "cover": "nyc --cache npm test; nyc report --reporter=html",
    "init-cloudsql": "node books/model-cloudsql.js"
  },
  "author": "Gennaro.",
  "contributors": [
    {
      "name": "gennaro",
      "email": "gennaro.laera@gmail.com"
    }
  ],
  "cloud-repo-tools": {
    "requiresKeyFile": true,
    "requiresProjectId": true,
    "test": {
      "app": {
        "args": [
          "app.js"
        ],
        "msg": "Bookshelf - Node.js on Google Cloud Platform"
      }
    }
  },
  "license": "Apache-2.0",
  "semistandard": {
    "globals": [
      "after",
      "afterEach",
      "before",
      "beforeEach",
      "describe",
      "it"
    ]
  },
  "dependencies": {
    "@google-cloud/datastore": "1.1.0",
    "@google-cloud/storage": "1.4.0",
    "@google-cloud/vision": "^0.14.0",
    "body-parser": "1.18.2",
    "connect-memcached": "0.2.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "lodash": "4.17.4",
    "mongodb": "2.2.33",
    "multer": "1.3.0",
    "mysql": "2.15.0",
    "nconf": "0.8.5",
    "passport": "0.4.0",
    "passport-google-oauth20": "1.0.0",
    "prompt": "1.0.0",
    "pug": "2.0.0-rc.4",
    "uglify-js": "3.1.6",
    "web-push": "^3.2.5"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "2.1.1",
    "ava": "0.23.0",
    "proxyquire": "1.8.0",
    "sinon": "4.0.2"
  },
  "engines": {
    "node": ">=4.3.2"
  }
}
